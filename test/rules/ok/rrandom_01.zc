(* We divide by zero, but the contexts we are using for random
   are still well-defined. *)

prove [
  x <-$ Fq;
  y <-$ Fq \ (1/x);  (* y - 1/x is not a polynomial, so we ignore this inequality *)
  z <-$ Fq \ x;      (* x is well-defined, so we add z - x <> 0  *)
  u <-$ Fq;
] : true.
rrandom 4 (w -> w/ (z - x)) (w -> w * (z - x)) l_u.
print_goals : t2.